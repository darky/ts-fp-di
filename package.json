{
  "name": "ts-fp-di",
  "version": "0.22.0",
  "description": "Tiny TypeScript functional dependency injection, based on AsyncLocalStorage. Supports Node.js, Deno",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:darky/ts-fp-di.git"
  },
  "keywords": [
    "fp",
    "functional",
    "di",
    "dependency",
    "injection",
    "typescript",
    "ts",
    "async",
    "local",
    "storage",
    "asynclocalstorage",
    "deno"
  ],
  "scripts": {
